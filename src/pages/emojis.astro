---
import Layout from '@layouts/CardLayout.astro'
import Card from '@components/CardEmojis.svelte'
import data from '@data/emoji-list-alpha.json'
const emojis = JSON.parse(JSON.stringify(data))
---

<Layout title="emojis" bgColor="#272727" >
    {Object.keys(emojis).map((name, i) => <Card client:visible idx={i} letter={emojis[name]} />)}
</Layout>
