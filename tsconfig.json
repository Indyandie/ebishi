{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ]
    },
    "plugins": [
      {
        "name": "typescript-svelte-plugin"
      }
    ]
  }
}