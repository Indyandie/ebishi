---
import Layout from "../layouts/Layout.astro";
import Card from "../components/Card.svelte";
import data from "../data//abc-emoji-list.json";
const abc = JSON.parse(JSON.stringify(data));
---


<Layout title="abc">
  <main>
    {Object.keys(abc).map((name) => <Card client:visible letter={abc[name]}/>)}
  </main>
</Layout>

<style>
  h1 {
    margin: 2rem 0;
  }

  main {
    margin: auto;
    padding: 0;
    display: flex;
    width: min-content;
    /* touch-action: none; */
  }
</style>
