---
import Layout from "../layouts/Layout.astro";
import Card from "../components/Card.astro";
import data from "../data//abc-emoji-list.json";
const abc = JSON.parse(JSON.stringify(data));
---

<Layout title="abc">
  <main>
    {Object.keys(abc).map((name, i) => <Card letter={abc[name][0]} index={i} />)}
    <!-- <Card letter={abc.a[0]} /> -->
  </main>
</Layout>

<style>
  h1 {
    margin: 2rem 0;
  }

  main {
    margin: auto;
    padding: 0;
    display: flex;
    width: min-content;
    touch-action: none;
  }
</style>
